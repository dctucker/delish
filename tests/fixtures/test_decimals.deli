#!/usr/bin/deli

$x = 0.123
$y = 123.45
$x = 456.78
$z = ( $x + $y )
if $z != 580.23 {
  out "addition failed: ", $z, " != 580.23"
  return 1
}

$z = ( $x * $y )
if $z != 56389.491 {
  out "multiplication failed: ", $z, " != 56389.4910"
  return 1
}

$x = 123.45
$y =   0.5678
$z = ( $x - $y )
if $z != 122.8822 {
  out "subtraction failed: ", $z, " != 122.8822"
  return 1
}

$z = ( $x / $y )
if $z != 217.418104966 {
  out "division failed: ", $z, " != 217.418104966"
  return 1
}
